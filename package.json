{
  "name": "react-css-copypaste",
  "publisher": "Huzaifa-Ahmed",
  "displayName": "React CSS",
  "description": "Replace traditional CSS with JavaScript Object CSS with just copy/paste",
  "version": "0.0.1",
  "github": "https://github.com/Huzaifaahmed20",
  "icon": "assets/icon.jpg",
  "repository": {
    "type": "github",
    "url": "https://github.com/Huzaifaahmed20/react-css"
  },
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "javascript",
    "reactjs",
    "css",
    "js-css",
    "react and css",
    "CSS in react",
    "typescript",
    "react extension",
    "snippets"
  ],
  "activationEvents": [
    "onCommand:react-css-copypaste.pasteCSS"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react-css-copypaste.pasteCSS",
        "title": "CSS Paste Successfully"
      }
    ],
    "keybindings": [
      {
        "command": "react-css-copypaste.pasteCSS",
        "key": "ctrl+shift+v",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.55.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "clipboardy": "^2.3.0"
  }
}